@Article{r24,
  author =                     {Liu, H. and Tao, L. and Shen, W. Z.},
  title =                      {{Optimal self-organized growth of small anodic TiO2 nanotubes with ``micro-annealing{''} effect under complex conditions via reaction-diffusion approach}},
  journal =                    {{ELECTROCHIMICA ACTA}},
  year =                       {{2011}},
  volume =                     {{56}},
  number =                     {{11}},
  pages =                      {{3905-3913}},
  month =                      {{APR 15}},
  abstract =                   {{This work investigates the change of the morphology and the structural
   characteristics of the anodic TiO2 nanotubes under complex conditions (
   the temperature and the applied potential difference). Optimal growth
   for small tubes with specific sizes has been realized during this
   investigation, for example, high quality uniform ultrasmall (outer
   diameter D similar to 10 nm, wall thickness d similar to 3 nm) tubes can
   be grown without sacrifice of the growth rate (0.3-0.4 mu m h(-1)). A
   mechanistic description to the nonmonotonous morphology change under low
   temperatures has been approached with reaction-diffusion method
   considering the local reaction, diffusion and field drift processes of
   the main reaction species. We have also investigated the details of the
   partial crystallization in the as-prepared TiO2 tubes and discovered its
   strong relationship with the temperature. Due to its similarity to the
   thermo annealing up to several hundred centigrade but much smaller
   length scale, this phenomenon is named as ``micro-annealing{''} effect.
