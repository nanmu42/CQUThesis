@Article{r22,
  author =                     {Zhu, Xufei and Song, Ye and Yu, Dongliang and Zhang, Changshan and Yao, Wei},
  title =                      {{A novel nanostructure fabricated by an improved two-step anodizing technology}},
  journal =                    {{ELECTROCHEMISTRY COMMUNICATIONS}},
  year =                       {{2013}},
  volume =                     {{29}},
  pages =                      {{71-74}},
  month =                      {{APR}},
  abstract =                   {{A novel two-tier nanostructure with narrow mouth and wide abdomen in
   porous anodic alumina (PAA) has been successfully fabricated by an
   improved two-step anodization in oxalic acid (H2C2O4) and phosphoric
   acid (H3PO4) solutions. The two-tier nanostructure is clarified by oxide
   flow model and oxygen bubble mold. The small pores result from oxygen
   evolution at low critical voltage of similar to 97 V in 2 wt.\% H2C2O4
   solution, the big pores result from oxygen evolution at high critical
   voltage of similar to 162 V in 2 wt.\% H3PO4 solution. Electronic
   current across the barrier layer formed in H2C2O4 is greater than that
   in H3PO4. This is the primary reason why the porosity of PAA formed in
   H2C2O4 is greater than that in H3PO4. (c) 2013 Elsevier B.V. All rights
