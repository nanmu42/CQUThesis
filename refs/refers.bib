@Article{r49,
  author =                     {Chen, Chien-Chon and Chung, Hsien-Wen and Chen, Chin-Hsing and Lu, Hsuch-Pei and Lan, Chi-Ming and Chen, Si-Fan and Luo, Liyang and Hung, Chen-Shiung and Diau, Eric Wei-Guang},
  title =                      {{Fabrication and Characterization of Anodic Titanium Oxide Nanotube Arrays of Controlled Length for Highly Efficient Dye-Sensitized Solar Cells}},
